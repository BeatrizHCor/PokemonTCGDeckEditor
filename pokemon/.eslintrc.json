{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended", 
        "standard", 
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", "prettier"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "prettier/prettier": "error"
    }
}
